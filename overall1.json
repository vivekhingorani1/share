  "paths": {
    "/api/v1/events": {
      "get": {
        "tags": [
          "Events"
        ],
        "summary": "Get a list of events from SalesLogix event store",
        "description": "Event Types: \r\n* ClientCreated, ClientUpdated\r\n* RelationshipCreated, RelationshipUpdated",
        "parameters": [
          {
            "name": "start",
            "in": "query",
            "description": "The last event id retrieved",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 1
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "The number of events to retrieve",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 10
            }
          },
          {
            "name": "accountId",
            "in": "query",
            "description": "The SalesLogix AccountId",
            "schema": {
              "type": "string",
              "default": ""
            }
          }
        ],
        "responses": {
          "200": {
            "description": "If data returned",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EventModel"
                }
              }
            }
          },
          "204": {
            "description": "If no data returned"
          },
          "408": {
            "description": "If the request timed out",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/repositories/entity": {
      "get": {
        "tags": [
          "Repositories"
        ],
        "summary": "Get entity structure",
